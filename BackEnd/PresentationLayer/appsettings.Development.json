{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },

  // Development settings - Secrets loaded from User Secrets
  // Run: dotnet user-secrets list
  // To set: dotnet user-secrets set "Key:SubKey" "Value"

  "DetailedErrors": true,

  // Default development values (override with user-secrets)
  "Redis": {
    "Host": "localhost",
    "Port": "6379"
  }
}
